
/*****************业务样式*******************/
.top-bar-style {
    /* border-style: solid;
    border-color: rgb(0, 140, 255);
    border-width: 5rpx; */
}

.top-bar-child-style {
    border-bottom: 1px solid rgb(0, 140, 255);
    border-radius: 20rpx;
}

.bottom-bar-style {
    /* 将图片转化为base64 */
    background-image: url('data:image/jpeg;base64,');
    background-size: auto; /* 图片的原始大小 */
    background-position: center; /* 图片在元素中间 */
    background-repeat: no-repeat; /* 图片不重复 */
}

.top-bar {
    height: 30%;
    animation: slideup 1s forwards;
}

.bottom-bar {
    height: 70%;
    animation: slideup 3s forwards;
}

.name-filter-label {
    width: 15%;
}

.name-filter-input {
    width: 85%;
}

.name-input-box {
    margin-left: -80rpx;
    margin-top: 10rpx;
}

.rssi-label {
    width: 15%;
}

.rssi-slider {
    width: 70%;
}

.rssi-value {
    width: 15%;
}

.scan-button {
    width: 20%;
    height: 70%;
    border-style: solid;
    border-width: 2px;
    border-color: rgb(10, 181, 211);
    border-radius: 50rpx;
    background-color: rgb(0, 153, 255);
}

.scan-button:active {
    background-color: rgb(35, 126, 187);
}

.device-item-container {
    height: 25%;
}

.device-item {
    height: 80%;
    width: 90%;
}

.device-item-name {
    width: 80%;
}

.device-item-rssi {
    width: 20%;
}
